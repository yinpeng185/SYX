page {
  height: 100%;
  position: relative;
  overflow: hidden;
  box-sizing: border-box;
}

.view-page {
  height: 100%;
  width: 100%;
  left: 0;
  top: 0;
  box-sizing: border-box;
  position: absolute;
  overflow: hidden;
}

.page-content {
  box-sizing: border-box;
  position: relative;
  overflow-y: auto;
  background-color: #f1f4f6;
  height: 100%;
}
@import "../public/public.wxss";
/* 内容 */
.content {
  margin: 0 1%;
}

.item-view {
