.image-close {
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #fff;
  background: rgba(0, 0, 0, 0.2);
  position: absolute;
  top: 0;
  right: 0;
}

.image-close::before, .image-close::after {
  content: '';
  width: 50%;
  height: 1px;
  background: #e5e5e5;
  position: absolute;
  top: 50%;
  left: 50%;
}

.image-close::before {
  transform: translate(-50%, -50%) rotate(45deg);
}

.image-close::after {
  transform: translate(-50%, -50%) rotate(135deg);
}

